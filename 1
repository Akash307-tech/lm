1.import pandas as pd import numpy as np;; data=pd.read_csv("tennis.csv") print(data) 
d=np.array(data)[:,:-1] print("the attributes are: ",d) ;;     target= np.array(data)[:,-1] print("the target is :",target) ;;    def train(d, t):
         specific_hypothesis = None
         for i, val in enumerate(t):
                     if val == "yes":
                          specific_hypothesis = d[i].copy()
                          break
       if specific_hypothesis is None:  
                return "No positive example found in the target"
      for i, val in enumerate(d):
 if t[i] == "yes":
       for x in range(len(specific_hypothesis)):
if val[x] != specific_hypothesis[x]:
        specific_hypothesis[x] = '?'
      return specific_hypothesis  ;;
print("the final hypothesis is :",train(d,target))
